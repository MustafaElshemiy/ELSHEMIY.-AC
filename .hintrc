{
  "extends": [
    "development"
  ],
  "hints": {
    "meta-viewport": "off",
    "meta-charset-utf-8": "off",
    "axe/name-role-value": "off",
    "axe/structure": [
      "default",
      {
        "list": "off",
        "listitem": "off"
      }
    ],
    "axe/text-alternatives": [
      "default",
      {
        "image-alt": "off",
        "document-title": "off"
      }
    ],
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "backdrop-filter",
          "user-select",
          "float: inline-end"
        ]
      }
    ],
    "axe/forms": [
      "default",
      {
        "label": "off",
        "select-name": "off"
      }
    ],
    "button-type": "off",
    "disown-opener": "off"
  }
}